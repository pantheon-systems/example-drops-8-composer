{
  "name": "jcalderonzumba/mink-phantomjs-driver",
  "description": "PhantomJS driver for Mink framework",
  "keywords": [
    "phantomjs",
    "headless",
    "javascript",
    "ajax",
    "testing",
    "browser"
  ],
  "homepage": "http://mink.behat.org/",
  "type": "mink-driver",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Francisco CalderÃ³n Zumba",
      "email": "juanfcz@gmail.com",
      "homepage": "http://github.com/jcalderonzumba"
    }
  ],
  "require": {
    "php": ">=5.4",
    "behat/mink": "~1.7",
    "twig/twig": "~1.20|~2.0",
    "jcalderonzumba/gastonjs": "~1.0"
  },
  "require-dev": {
    "mink/driver-testsuite": "dev-master",
    "phpunit/phpunit": "~4.6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Zumba\\Mink\\Driver\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Behat\\Mink\\Tests\\Driver\\": "tests/integration"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
